# BEECROWD--EVERYTHIMG
#include<stdio.h>
int main()
{
    int n,i,x,f,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        scanf("%d",&x);
        f = 0;
        for(j=2;j<x;j++)
        {
            if(x%j==0)
            {
                f = 1;
            }
        }
        if(f==1 || x==1)
    {
        printf("%d nao eh primo\n",x);
    }
    else
    {
        printf("%d eh primo\n",x);
    }

    }
    return 0;

}
