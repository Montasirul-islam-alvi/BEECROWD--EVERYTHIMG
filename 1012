#include<stdio.h>
int main()
{
    double a,b,c,areatriangle,circlearea,pi,trapeziumarea,areaofsquare,rectanglearea;
    scanf("%lf %lf %lf",&a,&b,&c);
    areatriangle = (1/2.0)*a*c;
    printf("TRIANGULO: %.3lf\n",areatriangle);

    pi = 3.14159;
    circlearea = pi*c*c;
    printf("CIRCULO: %.3lf\n",circlearea);

    trapeziumarea = (1/2.0)*(a+b)*c;
    printf("TRAPEZIO: %.3lf\n",trapeziumarea);

    areaofsquare = b*b;
    printf("QUADRADO: %.3lf\n",areaofsquare);

    rectanglearea = a*b;
    printf("RETANGULO: %.3lf\n",rectanglearea);
    return 0;

}
