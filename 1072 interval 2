# BEECROWD--EVERYTHIMG
#include<stdio.h>
int main()
{
    int n,i,x,countin=0,countout=0;
    scanf("%d",&n);
    i=1;
    while(i<=n)
    {
        scanf("%d",&x);
        if(x>=10&&x<=20)
        {
            countin++;
        }
        else
        {
             countout++;
        }
        i++;
    }
    printf("%d in\n",countin);
    printf("%d out\n",countout);
    return 0;
}
