# BEECROWD--EVERYTHIMG
#include<stdio.h>
int main()
{
    int x,sum = 0,count = 0;
    double avrg;
    while(1)
    {
        scanf("%d",&x);
        if(x<0)
        {
            break;
        }
        else
        {
            sum += x;
            count++;
        }
    }
    avrg = (double)sum/count;
    printf("%.2lf\n",avrg);
    return 0;
}
